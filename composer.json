{
    "name": "biscolab/google-maps-php-sdk",
    "description": "Google Maps PHP SDK.",
    "license": "MIT",
    "version": "0.9.2",
    "keywords": [
        "google",
        "maps",
        "geocoding",
        "elevation",
        "places",
        "roads"
    ],
    "authors": [
        {
            "name": "Roberto Belotti",
            "email": "roby.belotti@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "5.*|6.*|7.*"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "7.*|8.*|^9.0",
        "psr/log": "^1.0",
        "phpdocumentor/reflection-docblock": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Biscolab\\GoogleMaps\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Biscolab\\GoogleMaps\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-no-http": "phpunit --exclude-group http"
    }
}
